{
  "name": "@gud/bench",
  "version": "1.1.0",
  "description": "A powerful and elegant benchmarking tool for JavaScript and TypeScript.",
  "license": "Apache-2.0",
  "author": "Ryan Goree (https://github.com/ryangoree)",
  "repository": "https://github.com/ryangoree/gud-bench",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "bench": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "typescript": ">5"
  },
  "dependencies": {
    "@gud/math": "^1.0.0",
    "@gud/cli": "^1.0.0-beta.7",
    "@gud/cli-menu": "^1.0.0-beta.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@changesets/cli": "^2.29.4",
    "@types/node": "^24.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc --watch",
    "bench": "node dist/cli.js",
    "test": "tsx --test tests/*.test.ts",
    "format": "biome format --write",
    "lint": "biome lint --diagnostic-level error .",
    "lint:fix": "npm:lint --fix",
    "lint:fix:unsafe": "npm:lint:fix --unsafe",
    "typecheck": "tsc --noEmit",
    "release": "npm run build && changeset publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
