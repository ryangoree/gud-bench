{
  "name": "@gud/bench",
  "version": "1.1.1",
  "description": "A powerful and elegant benchmarking tool for JavaScript and TypeScript.",
  "license": "Apache-2.0",
  "author": "Ryan Goree (https://github.com/ryangoree)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryangoree/gud-bench.git"
  },
  "sideEffects": false,
  "type": "module",
  "bin": {
    "bench": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "tsc --build --clean tsconfig.cli.json tsconfig.lib.json",
    "build": "tsc --build tsconfig.cli.json tsconfig.lib.json",
    "watch": "tsc --build tsconfig.cli.json tsconfig.lib.json --watch",
    "bench": "node dist/cli.js",
    "test": "tsx --test tests/*.test.ts",
    "format": "biome format --write",
    "lint": "biome lint --diagnostic-level error .",
    "lint:fix": "npm:lint --fix",
    "lint:fix:unsafe": "npm:lint:fix --unsafe",
    "typecheck": "tsc --noEmit",
    "release": "npm run build && changeset publish"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@gud/cli": "^1.0.0-beta.7",
    "@gud/cli-menu": "^1.0.0-beta.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "imports": {
    "#src/*": "./dist/*.js"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
